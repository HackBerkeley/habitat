<% layout('layout') -%>
<div id="content">
  <!-- Blank column -->
  <div class="grid column1" id="links">
    <% for (var i=0; i<5; i++) { %>
      <div class="blank square onecol"></div>
    <% } %>
  </div>
  <!-- Hacks -->
  <div class="grid column6" id="links">
    <% var squarecount = 0; %>
    <% for (var i=0; i<hacks.length; i++) { squarecount++; %>
    <div class="square link sixcol" name=<%= "projects/" + hacks[i].hackid %>>
      <span><%= hacks[i].title %></span>
      <% if (hacks[i].picture) { %>
        <object data="<%= hacks[i].picture %>" style="width:100%">
          <img src="https://s3.amazonaws.com/hackerfair/hack-logo-black-png.png" style="width: 100%;">
        </object>
      <% } else { %>
        <img src="https://s3.amazonaws.com/hackerfair/hack-logo-black-png.png" style="width: 100%;">
      <% } %>
      <div class="squaretext"></div>
    </div>
    <% } %>
<% var remainder = ((squarecount < 30) ? 30 : 6*(Math.floor(squarecount / 6) + (squarecount % 6 == 0 ? 0 : 1))) - squarecount; %>
<% for (var i=0; i<remainder; i++) { %>
  <div class="blank square sixcol"></div>
<% } %>
  </div>
  <!-- Empty last col 2nd 2 rows-->
  <div class="grid column1" id="links">
    <% for (var i=0; i<5; i++) { %>
      <div class="blank square onecol"></div>
    <% } %>
  </div>
</div>
